{
    "name": "jakyeru/laravel-shell",
    "description": "Laravel-Shell adds a terminal interface to your web application, allowing you to run shell commands directly from your browser.",
    "type": "library",
    "require": {
        "php": "^8.2|^8.3",
        "laravel/framework": "^11",
        "livewire/livewire": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jakyeru\\LaravelShell\\LaravelShellServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jakyeru\\LaravelShell\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jakye",
            "email": "jakye@jakye.me"
        }
    ],
    "minimum-stability": "dev"
}
